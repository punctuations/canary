{
	"name": "dsgn",
	"description": "An ESM node package template with ESLint, Prettier & TypeScript built in. Powered by esbuild.",
	"version": "1.0.0",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/punctuations/dsgn.git"
	},
	"author": "Matt <matt@dont-ping.me> (https://dont-ping.me)",
	"license": "MIT",
	"devDependencies": {
		"@types/react": "^17.0.26",
		"@types/styled-components": "^5.1.14",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"eslint": "^7.32.0",
		"eslint-config-xo": "^0.38.0",
		"eslint-config-xo-typescript": "^0.44.0",
		"framer-motion": "^4.1.17",
		"prettier": "^2.4.1",
		"react": "^17.0.2",
		"styled-components": "^5.3.1",
		"tsup": "^5.2.1",
		"typescript": "^4.4.3"
	},
	"scripts": {
		"build": "tsup src/index.ts --dts --format esm,cjs",
		"lint": "eslint ./src/**/*.{ts,tsx}",
		"fix": "eslint ./src/**/*.{ts,tsx} --fix"
	},
	"bugs": {
		"url": "https://github.com/alii/nextkit/issues"
	},
	"homepage": "https://github.com/alii/nextkit#readme",
	"files": [
		"dist",
		"package.json"
	],
	"keywords": [
		"node",
		"template"
	]
}
