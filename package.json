{
	"name": "dsgn",
	"description": "A compact & modern ui kit.",
	"version": "1.0.0",
	"main": "dist/index.js",
	"module": "esm/index.js",
	"types": "dist/index.d.ts",
	"unpkg": "dist/index.min.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/punctuations/dsgn.git"
	},
	"author": "Matt <matt@dont-ping.me> (https://dont-ping.me)",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/core": "^7.15.8",
		"@babel/plugin-transform-runtime": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.15.0",
		"@rollup/plugin-commonjs": "^21.0.0",
		"@rollup/plugin-node-resolve": "^13.0.5",
		"@types/node": "^14.14.16",
		"@types/react": "^17.0.26",
		"@typescript-eslint/eslint-plugin": "^4.32.0",
		"@typescript-eslint/parser": "^4.32.0",
		"eslint": "^7.32.0",
		"eslint-config-xo": "^0.38.0",
		"eslint-config-xo-typescript": "^0.44.0",
		"framer-motion": "^4.1.17",
		"fs-extra": "^10.0.0",
		"prettier": "^2.4.1",
		"react": "^17.0.2",
		"rollup": "^2.58.0",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-local-resolve": "^1.0.7",
		"styled-jsx": "^4.0.1",
		"typescript": "^4.4.3"
	},
	"scripts": {
		"release": "rollup -c && tsc",
		"build:rollup": "rollup -c",
		"build:esm": "babel --config-file ./babel.config.js --extensions \".js,.ts,.tsx\" ./src --out-dir ./esm --ignore \"**/__tests__/**/*,**/*.d.ts\"",
		"build:types": "tsc",
		"build:esm-types": "tsc --outDir ./esm",
		"clear": "rm -rf ./dist ./esm",
		"lint": "eslint ./src/**/*.{ts,tsx}",
		"fix": "eslint ./src/**/*.{ts,tsx} --fix"
	},
	"bugs": {
		"url": "https://github.com/punctuations/dsgn/issues"
	},
	"homepage": "https://github.com/punctuations/dsgn#readme",
	"files": [
		"dist",
		"package.json"
	],
	"keywords": [
		"react",
		"components",
		"ui",
		"design",
		"ui-kit"
	]
}
